<div class="booyah-box col-10 offset-1">
  <h1> <%= @place.name %> </h1>
  <br />
  <i><%= @place.address %></i>
  <br /><br />
  <p><%= @place.description %></p>


  <div class="float-right">
    <%= link_to 'Edit', edit_place_path(@place), class: 'btn btn-primary' %>
  </div>
</div>
<!-- once the show action is called, it passes in place[:id] to here and we then display it however we want -->