<!-- <h1><%= @places.inspect %></h1> -->



<% @places.each do |place| %>
  <div class="booyah-box col-10 offset-1">  <!-- 10 cols wide and offset by 1 so always in center-->
    <h1><%= place.name %></h1>
    <i><%= place.address %></i><br /><br />
    <p><%= place.description %></p><br />
  </div>

<% end %>